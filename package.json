    "start": "react-app-rewired start",
    "build": "(export REACT_APP_SENTRY_RELEASE=$(git rev-parse --short HEAD); react-app-rewired build)",
    "upload-sentry": "(export REACT_APP_SENTRY_RELEASE=$(git rev-parse --short HEAD); node scripts/sentry-release.js)",
    "release": "npm run build && npm run upload-sentry",
    "release-windows": "for /f %i in ('git rev-parse --short HEAD') do set REACT_APP_SENTRY_RELEASE=%i && yarn run build && node scripts/sentry-release.js",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
